{
    "compilerOptions": {
        "target": "ES5",
        "module": "ES6",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "jsx": "react",
        "jsxFactory": "createElement",
        "skipLibCheck": true,
        "lib": ["ES2019", "DOM", "DOM.Iterable"]
    },
    "include": ["source/*"],
    "typedocOptions": {
        "name": "Crank Router",
        "mode": "file",
        "excludeExternals": true,
        "excludeNotExported": true,
        "readme": "./ReadMe.md",
        "out": "docs/"
    }
}
